	<aura:handler name="init" value="{!this}" action="{!c.doInit}" />
	<aura:if isTrue="{!v.pageReady}">
		<aura:if isTrue="{!and(v.validationError, v.readOnlyMode)}">
			<div style="color: red">
				<center><b>All required information has not been provided. Please complete and/or correct your biographic information.</b></center>
			</div>
		</aura:if>
		
		<div class="slds-grid slds-gutters">
			
			<div class="slds-col slds-size_1-of-2"><span>
					<h2>Identity Information</h2>
					<c:gDisplayContent uniqueContentValue="EpicBiographicNote1">
				    </c:gDisplayContent>
					<lightning:input name="lastName" value="{!v.contact.LastName}" placeholder="" label="Last Name/Surname"
						aura:id="fieldToValidate" required="true" disabled="{!v.readOnlyMode}" />
					<lightning:input type="checkbox" label="My legal name consists of one name only" name="lastOnly"
						checked="{!v.contact.legal_name_consists_of_one_name_only__c}" disabled="{!v.readOnlyMode}" />
					<aura:if isTrue="{!v.pageReady}">
						<aura:if isTrue="{!!v.contact.legal_name_consists_of_one_name_only__c}">
							<lightning:input name="firstName" value="{!v.contact.FirstName}" placeholder=""
								label="Rest of Name" aura:id="fieldToValidate"
								required="{!!v.contact.legal_name_consists_of_one_name_only__c}"
								disabled="{!v.readOnlyMode}" onblur="{!c.handleInputOnBlur}" />
						</aura:if>
					</aura:if>
					<lightning:select name="generationalSuffix" label="Generational Suffix"
						value="{!v.contact.Generational_Suffix__c}" disabled="{!v.readOnlyMode}">
						<aura:iteration items="{!v.generationalSuffix}" var="genSuf">
							<option text="{!genSuf}"></option>
						</aura:iteration>
					</lightning:select>
					<lightning:select name="gender" label="Gender" aura:id="fieldToValidate" required="true"
						value="{!v.contact.Gender__c}" disabled="{!v.readOnlyMode}">
						<aura:iteration items="{!v.gender}" var="gen">
							<option text="{!gen}"></option>
						</aura:iteration>
					</lightning:select>
					<c:gDisplayContent uniqueContentValue="EPICBiographicIdentityInfoNote2">
				    </c:gDisplayContent>					
				
					<lightning:input name="dateOfBirth" type="date" value="{!v.contact.Birthdate}" placeholder=""
						label="Date of Birth" aura:id="fieldToValidate" required="true" disabled="{!v.readOnlyMode}"
						onchange="{!c.checkDOBIfValid}" />
					<aura:if isTrue="{!v.pageReady}">
						<lightning:combobox name="passportCountry" label="Passport Country"
							messageWhenValueMissing="Choose one!" value="{!v.contact.Passport_Country__c}"
							options="{!v.passportCountryOptions}" aura:id="fieldToValidate" required="true"
							disabled="{!v.readOnlyMode}">
							<aura:iteration items="{!v.currentCitizenshipCountry}" var="co">
								<option text="{!co}" selected="{!co == v.contact.Passport_Country__c}"></option>
							</aura:iteration>
						</lightning:combobox>
					</aura:if>
					<lightning:input name="passportNumber" value="{!v.contact.Passport_Number__c}" placeholder=""
						label="Passport Number" aura:id="fieldToValidate" required="true" disabled="{!v.readOnlyMode}"
						onblur="{!c.handleInputOnBlur}" />
					<lightning:input name="passportIssueDate" type="date" value="{!v.contact.Passport_Issue_Date__c}"
						placeholder="" label="Passport Issue Date" aura:id="fieldToValidate" required="true"
						disabled="{!v.readOnlyMode}" onchange="{!c.checkIssueDateIfValid}" />
					<lightning:input name="passportExpiration" type="date" value="{!v.contact.Passport_Expiration__c}"
						placeholder="" label="Passport Expiration Date" aura:id="fieldToValidate" required="true"
						disabled="{!v.readOnlyMode}" onchange="{!c.checkExpirationDateIfValid}" />
					<aura:if isTrue="{!!v.readOnlyMode}">
						<p><span class="redText">*</span>Upload Photo</p>
						<!-- UX Note: we should not display this as an error since the user has not yet made a mistake, suggest we come back to this and use the branded "Warning" styles once finalized -->
							<c:gDisplayContent uniqueContentValue="EPICBiographicIdentityInfoNote3">
				            </c:gDisplayContent>
						<c:cloudDocumentUploadWrapper aura:id="photo" payLoad="{!v.payloadPhoto}"
							maxAllowedFileSize="{!v.maxsize}" acceptedFileFormats=".jpeg,.jpg" enableAnnotation="false"
							enableRedaction="false" assetType="PHOTO"
							onuploadcompleted="{!c.handleUploadFinishedPhoto}" />		
						<c:gDisplayContent uniqueContentValue="upload_msg_jpeg_jpg">
						</c:gDisplayContent>
						<aura:set attribute="else">
							Photo Preview
							<c:cloudDocumentUploadWrapper aura:id="photo" payLoad="{!v.payloadPhoto}"
								maxAllowedFileSize="{!v.maxsize}" acceptedFileFormats=".jpeg,.jpg"
								hideUpload="{!v.upload}" enableAnnotation="false" enableRedaction="false"
								assetType="PHOTO" onuploadcompleted="{!c.handleUploadFinishedPhoto}" /><br />
						</aura:set>
					</aura:if>
					
					<aura:if isTrue="{!!v.readOnlyMode}">
						<p><span class="redText">*</span>Upload Passport</p>
						<c:gDisplayContent uniqueContentValue="EpicBioIdentityInfoNote4">
				            </c:gDisplayContent>
						<c:cloudDocumentUploadWrapper aura:id="passport" payLoad="{!v.payloadPassport}"
							maxAllowedFileSize="{!v.maxsize}" acceptedFileFormats=".jpeg,.jpg" enableAnnotation="false"
							enableRedaction="false" assetType="PASSPORT"
							onuploadcompleted="{!c.handleUploadFinishedPassport}" />
							<c:gDisplayContent uniqueContentValue="upload_msg_jpeg_jpg">
						</c:gDisplayContent>
						<aura:set attribute="else">
							Passport Preview
							<c:cloudDocumentUploadWrapper aura:id="passport" payLoad="{!v.payloadPassport}"
								maxAllowedFileSize="{!v.maxsize}" acceptedFileFormats=".jpeg,.jpg"
								enableAnnotation="false" hideUpload="{!v.upload}" enableRedaction="false"
								assetType="PASSPORT" onuploadcompleted="{!c.handleUploadFinishedPassport}" /><br />
						</aura:set>
					</aura:if>
						<c:gDisplayContent uniqueContentValue="PleaseNoteOfPassportPreview">
				    </c:gDisplayContent>											 		
					<lightning:input type="radio" label="Yes" name="isExp" value="true" disabled="{!v.readOnlyMode}"
						checked="{!v.contact.Passport_Photo_Includes_Expiration_Date__c}"
						onchange="{!c.handleExpirationChecked}" />
					<lightning:input type="radio" label="No" name="isExp" value="false" disabled="{!v.readOnlyMode}"
						checked="{!or(v.contact.Passport_Photo_Includes_Expiration_Date__c == false, or(v.contact.Passport_Photo_Includes_Expiration_Date__c == 'false', v.contact.Passport_Photo_Includes_Expiration_Date__c == null))}"
						onchange="{!c.handleExpirationChecked}" />
					<aura:if
						isTrue="{!and(or(v.readOnlyMode == false, v.readOnlyMode == true),  or(v.passport_Photo_Includes_Expiration_Date == 'false',or(v.passport_Photo_Includes_Expiration_Date == false, v.passport_Photo_Includes_Expiration_Date == null)))}">
						<aura:if isTrue="{!v.readOnlyMode == false}">
							<p><span class="redText">*</span>Upload Passport Expiration</p>
							<aura:if
							isTrue="{!and(v.passportExpirationDone != 'Yes', or(v.passport_Photo_Includes_Expiration_Date == false,or(v.passport_Photo_Includes_Expiration_Date == 'false', v.passport_Photo_Includes_Expiration_Date == null)))}">
							<c:gDisplayContent uniqueContentValue="EPICBioPassportExpiration">
				            </c:gDisplayContent>
						</aura:if>
							<c:cloudDocumentUploadWrapper aura:id="passportExpiration"
								payLoad="{!v.payloadPassportExpiration}" maxAllowedFileSize="{!v.maxsize}"
								acceptedFileFormats=".jpeg,.pdf,.tiff" enableAnnotation="false" enableRedaction="false"
								onuploadcompleted="{!c.handleUploadFinishedPassportExpiration}" />
								<c:gDisplayContent uniqueContentValue="upload_msg_jpeg_tiff_pdf">
						</c:gDisplayContent>
						</aura:if>
						<aura:if isTrue="{!v.readOnlyMode == true}">
							<c:cloudDocumentUploadWrapper aura:id="passportExpiration"
								payLoad="{!v.payloadPassportExpiration}" maxAllowedFileSize="{!v.maxsize}"
								acceptedFileFormats=".jpeg,.pdf,.tiff" enableAnnotation="false" enableRedaction="false"
								hideUpload="{!v.upload}"
								onuploadcompleted="{!c.handleUploadFinishedPassportExpiration}" />
						</aura:if>
						
					</aura:if>
						<c:gDisplayContent uniqueContentValue="CommunityBiographicExpirationNote">
						</c:gDisplayContent>
					<lightning:input type="radio" label="Yes" name="isEng" value="true" disabled="{!v.readOnlyMode}"
						checked="{!v.contact.Passport_Photo_Is_In_English__c}" onchange="{!c.handleEnglishChecked}" />
					<lightning:input type="radio" label="No" name="isEng" value="false" disabled="{!v.readOnlyMode}"
						checked="{!or(v.contact.Passport_Photo_Is_In_English__c == false, or(v.contact.Passport_Photo_Is_In_English__c == 'false', v.contact.Passport_Photo_Is_In_English__c == null))}"
						onchange="{!c.handleEnglishChecked}" />
					<aura:if
						isTrue="{!and(or(v.readOnlyMode == false, v.readOnlyMode == true), or(v.Passport_Photo_Is_In_English == 'false', or(v.Passport_Photo_Is_In_English == false, v.Passport_Photo_Is_In_English == null)))}">
						<aura:if isTrue="{!v.readOnlyMode == false}">
							<p>Upload Passport Translation</p>
							<aura:if
							isTrue="{!and(v.passportTranslationDone != 'Yes', or(v.Passport_Photo_Is_In_English == false,or(v.Passport_Photo_Is_In_English == 'false', v.Passport_Photo_Is_In_English == null)))}">
							<c:gDisplayContent uniqueContentValue="latin_char_desc">
				            </c:gDisplayContent>
						</aura:if>
							<c:cloudDocumentUploadWrapper aura:id="passportTranslation"
								payLoad="{!v.payloadPassportTranslation}" maxAllowedFileSize="{!v.maxsize}"
								acceptedFileFormats=".jpeg,.pdf,.tiff" enableAnnotation="false" enableRedaction="false"
								onuploadcompleted="{!c.handleUploadFinishedPassportTranslation}" />
								<c:gDisplayContent uniqueContentValue="upload_msg_jpeg_tiff_pdf">
						</c:gDisplayContent>
						</aura:if>
						<aura:if isTrue="{!v.readOnlyMode == true}">
							<c:cloudDocumentUploadWrapper aura:id="passportTranslation"
								payLoad="{!v.payloadPassportTranslation}" maxAllowedFileSize="{!v.maxsize}"
								acceptedFileFormats=".jpeg,.pdf,.tiff" enableAnnotation="false" enableRedaction="false"
								onuploadcompleted="{!c.handleUploadFinishedPassportTranslation}"
								hideUpload="{!v.upload}" />
						</aura:if>
					</aura:if>
				</span></div>
			<div class="slds-col slds-size_1-of-2"><span>
					<h2>Contact Information</h2>
					<c:gDisplayContent uniqueContentValue="EpicBiographicNote2">
				    </c:gDisplayContent>
					<c:DynamicAddress aura:id="submitAddress" street="{!v.contact.MailingStreet}"
						city="{!v.contact.MailingCity}" state="{!v.contact.MailingStateCode}"
						postalCode="{!v.contact.MailingPostalCode}" country="{!v.contact.MailingCountryCode}"
						prohibitedCountriesMap="{!v.prohibitedCountriesMap}" readOnly="{!v.readOnlyMode}"
						requiredFields="country,city,street" />
					<lightning:input name="phone" type="tel" value="{!v.contact.Phone}" placeholder="" label="Phone"
						aura:id="fieldToValidate" required="true" disabled="{!v.readOnlyMode}"
						onblur="{!c.handleInputOnBlur}" />
					<lightning:select name="phoneType" label="Phone Type" disabled="{!v.readOnlyMode}"
						aura:id="fieldToValidate" required="true" value="{!v.contact.Phone_Type__c}">
						<aura:iteration items="{!v.phoneTypeOptions}" var="pt">
							<option text="{!pt}"></option>
						</aura:iteration>
					</lightning:select>
					<h2>Citizenship Information</h2>					
					<aura:if isTrue="{!v.pageReady}">
						<lightning:combobox name="birthCountry" label="Birth Country"
							value="{!v.contact.Birth_Country__c}" aura:id="fieldToValidate" required="true"
							options="{!v.birthCountryOptions}" disabled="{!v.readOnlyMode}">
							<aura:iteration items="{!v.birthCountry}" var="co">
								<option text="{!co}" selected="{!co == v.contact.Birth_Country__c}"></option>
							</aura:iteration>
						</lightning:combobox>
					</aura:if>
					<lightning:input name="birthCity" value="{!v.contact.Birth_City__c}" placeholder=""
						label="Birth City" disabled="{!v.readOnlyMode}" />
					<lightning:input name="birthState" value="{!v.contact.Birth_State_Province__c}" placeholder=""
						label="Birth State/Province" disabled="{!v.readOnlyMode}" />
					<aura:if isTrue="{!v.pageReady}">
						<lightning:combobox name="citizenshipAtBirthCountry" label="Citizenship at Birth"
							value="{!v.contact.Citizenship_at_Birth__c}" aura:id="fieldToValidate" required="true"
							options="{!v.countryAtBirthOptions}" disabled="{!v.readOnlyMode}">
							<aura:iteration items="{!v.citizenshipAtBirthCountry}" var="co">
								<option text="{!co}" selected="{!co == v.contact.Citizenship_at_Birth__c}"></option>
							</aura:iteration>
						</lightning:combobox>
					</aura:if>
					<lightning:select name="schoolCitizenship" label="Citizenship Upon Entering Medical School"
						aura:id="schoolCitizenship" value="{!v.contact.Citizenship_Upon_Entering_Medical_School__c}"
						disabled="{!v.readOnlyMode}">
						<aura:if isTrue="{!v.citizenshipAtMedSchoolIsNull}">
							<option text="None"></option>
						</aura:if>
						<aura:iteration items="{!v.citizenshipUponEnteringMedSchoolCountry}" var="co">
							<option text="{!co}"
								selected="{!co == v.contact.Citizenship_Upon_Entering_Medical_School__c}"></option>
						</aura:iteration>
					</lightning:select>
					<lightning:dualListbox aura:id="fieldToValidate" name="CurrentCitizenship"
						label="Current Citizenship" sourceLabel="Available Citizenship"
						selectedLabel="Current Citizenship" options="{!v.currentCitizenshipCountryDual}"
						value="{!v.countryDualSelected}" disabled="{!v.readOnlyMode}" required="true"
						onchange="{!c.handleCitizenshipChange}" />
						<c:gDisplayContent uniqueContentValue="citizenshipInformations">
				    </c:gDisplayContent>
					<br />
					<h2>Existing Service IDs</h2>
					<br />
					<p>
						<lightning:radioGroup aura:id="previouslyVerifiedRadioGroup" name="previouslyVerified"
							type="radio"
							label="Have you ever had your credentials verified by ECFMG's EICS service?"
							options="{! v.previouslyVerifiedOptions }" value="{!v.previouslyVerifiedProxy}"
							disabled="{!v.readOnlyMode}" onchange="{!c.handlePreviouslyVerifiedOnChange}" />
					</p>
					<lightning:radioGroup aura:id="previouslyAppliedRadioGroup" name="previouslyApplied" type="radio"
						label="Have you ever submitted an Application for ECFMG Certification and/or an application to ECFMG for examination?"
						options="{! v.previouslyAppliedOptions }" value="{!v.previouslyAppliedProxy}"
						disabled="{!v.readOnlyMode}" onchange="{!c.handlePreviouslyAppliedOnChange}" />
					<aura:if isTrue="{!v.Previous_ECFMG_Cert_or_Examination}">
						<lightning:input name="usmleId" value="{!v.contact.Applicant_Provided_USMLE_ID__c}"
							aura:id="fieldToValidate" disabled="{!v.readOnlyMode}" placeholder="" label="USMLE ID"
							pattern="[0-9]{8}" messageWhenPatternMismatch="Please enter a valid USMLE ID as 01234567"
							maxlength="8" onblur="{!c.handleUSMLEChange}" />
					</aura:if>
					<lightning:radioGroup name="previouslyEPIC" type="radio" label="Have you ever used ECFMG's EPIC service?"
						options="{! v.previouslyEPICOptions }" value="{!v.previouslyEPICProxy}"
						disabled="{!v.readOnlyMode}" onchange="{!c.handlePreviouslyEPICOnChange}" />
					<aura:if isTrue="{!v.Previous_EPIC_Services}">
						<lightning:input name="epicId" value="{!v.contact.Applicant_Provided_EPIC_ID__c}"
							disabled="{!v.readOnlyMode}" placeholder="" label="EPIC ID" />
					</aura:if>
				</span></div>
		</div><br />
		<aura:if isTrue="{!!v.readOnlyMode}">
			<center>
				<aura:if isTrue="{!v.showBackToSummary == true}">
					<lightning:button disabled="{!v.disabled || v.disableButtons}" variant="brand" name="save"
						label="Save and Return" onclick="{!c.handleSaveAndReturn}" />
					&nbsp;
					<lightning:button aura:id="backToSummaryButton" variant="brand" name="summary" label="Cancel"
						onclick="{!c.handleCancelAndReturn}" disabled="{!v.disableButtons}" />
				</aura:if>
				<aura:if isTrue="{!v.showBackToSummary == false}">
					<lightning:button variant="brand" name="next" label="Previous" onclick="{!c.handlePrevious}"
						disabled="{!v.disableButtons}" />&nbsp;&nbsp;&nbsp;
					&nbsp;
					<lightning:button disabled="{!v.disabled || v.disableButtons}" variant="brand" name="save"
						label="Save Changes" onclick="{!c.handleSave}" />
					&nbsp;
					<lightning:button variant="brand" name="next" label="Next" onclick="{!c.handleNext}"
						disabled="{!v.disableButtons}" />
				</aura:if>
			</center>
		</aura:if>
	</aura:if>
	<aura:if isTrue="{!v.showExistingAccount}">
		<div class="demo-only" style="height: 640px;">
			<section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true"
				aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
				<div class="slds-modal__container">
					<header class="slds-modal__header">
						<button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse"
							title="Close">
							<span class="slds-assistive-text">Close</span>
						</button>
						<h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">Use Your Existing Account</h2>
					</header>
					<div class="slds-modal__content slds-var-p-around_medium" id="modal-content-id-1">
						<p>							
							<c:gDisplayContent uniqueContentValue="EPICBioFinalScreen">
				           </c:gDisplayContent>
						</p>
						<p>	
							<c:gDisplayContent uniqueContentValue="EpicBioFinallMessage">
				           </c:gDisplayContent>
						</p>
					</div>
					<footer class="slds-modal__footer">
						<lightning:button aura:id="okButton" class="slds-button slds-button_brand"
							onclick="{!c.handleExistingAccountOkButtonOnClick}">Ok</lightning:button>
					</footer>
				</div>
			</section>
			<div class="slds-backdrop slds-backdrop_open"></div>
		</div>
	</aura:if>
</aura:component>